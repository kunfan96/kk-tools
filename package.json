{
    "name": "kk-tools",
    "version": "0.2.2",
    "author": "kunfan96",
    "description": "The library like lodash with typescript",
    "keywords": [
        "lodash",
        "tools-function",
        "typescript"
    ],
    "scripts": {
        "build": "npm run test && npm run build-only",
        "build-only": "rollup --config",
        "test": "jest --config ./jest.config.js",
        "publish-patch": "npm version patch && npm run build && npm publish --access=public",
        "publish-minor": "npm version minor && npm run build && npm publish --access=public"
    },
    "typings": "./types/*.d.ts",
    "main": "./lib/index.js",
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.11.0",
        "@rollup/plugin-typescript": "^5.0.2",
        "@types/jest": "^26.0.10",
        "babel-plugin-external-helpers": "^6.22.0",
        "jest": "^26.4.0",
        "rollup": "^2.26.3",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-uglify": "^6.0.4",
        "ts-jest": "^26.2.0"
    },
    "dependencies": {
        "tslib": "^2.0.1",
        "typescript": "^3.9.7"
    }
}
